{
    "name": "tolrac/cakephp5-sweet-alert-helper",
    "description": "SweetAlertHelper plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpunit/phpunit": "^10.5.5 || ^11.1.3"
    },
    "autoload": {
        "psr-4": {
            "SweetAlertHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SweetAlertHelper\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
